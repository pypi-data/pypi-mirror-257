    "def rpois(n,mu):\n",
    "    \"\"\"\n",
    "    Generates random variables from the Poisson distribution\n",
    "    \"\"\"\n",
    "    from scipy.stats import poisson\n",
    "    result=poisson.rvs(size=n,mu=mu)\n",
    "    return result"
   ]
